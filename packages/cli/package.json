{
  "name": "graphql-loadtest-cli",
  "version": "0.0.1",
  "description": "graphql-loadtest-cli CLI",
  "private": true,
  "types": "build/types/types.d.ts",
  "bin": {
    "graphql-loadtest-cli": "bin/graphql-loadtest-cli"
  },
  "scripts": {
    "format": "prettier --write **/*.{ts,json}",
    "lint": "eslint src/**/*",
    "clean-build": "rm -rf ./build",
    "compile": "tsc -p .",
    "build": "yarn format && yarn lint && yarn clean-build && yarn compile",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "watch": "jest --watch",
    "snapupdate": "jest --updateSnapshot",
    "coverage": "jest --coverage"
  },
  "files": [
    "tsconfig.json",
    "tslint.json",
    "build",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "gluegun": "latest",
    "graphql": "^14.6.0",
    "graphql-loadtest": "0.0.1-alpha.2",
    "inquirer": "../Inquirer.js/packages/inquirer",
    "time-span": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.11",
    "@types/prompts": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "jest": "^24.1.0",
    "prettier": "2.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "3.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
